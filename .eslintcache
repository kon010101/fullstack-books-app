Name=\"main-container\">\n          {/* <div className=\"logo\">\n            <b>\n              G<span>ro</span>up<span>2</span>\n              <span> </span>Lib<span>ra</span>ry\n            </b>\n          </div> */}\n          <div className=\"book-container\">\n            <ul className=\"list-inline\">\n              {contentfuls.map((book, idx) => {\n                return (\n                  <li className=\"book\">\n                    <img\n                      onClick={() => setShowInfos(idx)}\n                      src={book.fields.bookInfo.thumbnail}\n                    />\n                  </li>\n                );\n              })}\n              {showInfos > -1 ? (\n                <InfoContainer book={contentfuls[showInfos]} setShowInfos={setShowInfos}/>\n              ) : (\n                \"\"\n              )}\n            </ul>\n          </div>\n      </div>\n    </>\n  );\n}\n","/Users/korni/Documents/Arbeit/coding/React/20FullStackGroupWork/fullstack-book-app/src/components/ContentHelper.js",["46","47","48","49","50"],"import React, { useState, useEffect } from \"react\";\nimport TextField from \"@material-ui/core/TextField\";\nimport SearchOutlinedIcon from \"@material-ui/icons/SearchOutlined\";\nimport IconButton from \"@material-ui/core/IconButton\";\nimport axios from \"axios\";\nimport Card from \"./Card\";\nimport Header from \"./Header\";\nimport \"./ContentHelper.css\";\n\nfunction ContentHelper() {\n  const [maxResults, setMaxResults] = useState(40);\n  const [startIndex, setStartIndex] = useState(0);\n  const [query, setQuery] = useState(\"\");\n  const [loading, setLoading] = useState(\"\");\n  const [cards, setCards] = useState([]);\n\n  const fetchData = (add) => {\n    setLoading(true);\n\n    axios\n      .get(\n        `https://www.googleapis.com/books/v1/volumes?q=${query}&langRestrict=en&maxResults=${maxResults}&key=${process.env.REACT_APP_API_KEY}`\n      )\n      .then((res) => {\n        console.log(res.data.items);\n\n        setCards(res.data.items);\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n    setLoading(false);\n  };\n\n  const handleSubmitKeyDown = ({ keyCode }) => {\n    if (keyCode === 13) {\n      setStartIndex(1);\n      fetchData(false);\n    }\n  };\n  const handleSubmit = () => {\n    setStartIndex(1);\n    fetchData(false);\n  };\n\n  const handleLoadMore = () => {\n    //increase start index\n    setStartIndex((ind) => ind + 10);\n    fetchData(true);\n  };\n\n  return (\n    <>\n      <Header />\n      <div className=\"content-helper__container\">\n        <div className=\"content-helper\">\n          <h1 className=\"content-h1\">Google Books</h1>\n          <div className=\"search-container\">\n            <TextField\n              id=\"outlined-search\"\n              label=\"Search field\"\n              type=\"search\"\n              variant=\"outlined\"\n              className=\"search-field\"\n              onChange={(e) => setQuery(e.target.value)}\n              onKeyDown={handleSubmitKeyDown}\n              value={query}\n            />\n            <button\n              //   aria-label=\"delete\"\n              //   size=\"large\"\n              className={\"search-button\"}\n              onClick={handleSubmit}\n            >\n              <SearchOutlinedIcon />\n            </button>\n          </div>\n          <div className=\"card-container\">\n            {cards &&\n              cards.map((card, idx) => {\n                // console.log(idx, card.volumeInfo);\n                return (\n                  <Card\n                    card={card}\n                    image={\n                      card.volumeInfo.imageLinks &&\n                      card.volumeInfo.imageLinks.smallThumbnail\n                    }\n                  />\n                );\n              })}\n            {cards.length > 0 ? (\n              <button onClick={handleLoadMore}>Load More...</button>\n            ) : (\n              \"\"\n            )}\n          </div>\n        </div>\n      </div>\n    </>\n  );\n}\n\nexport default ContentHelper;\n","/Users/korni/Documents/Arbeit/coding/React/20FullStackGroupWork/fullstack-book-app/src/components/LandingPage.js",["51","52","53"],"import React from \"react\";\nimport { BrowserRouter as Router, Route, NavLink } from 'react-router-dom';\nimport \"./LandingPage.css\";\nimport Main from './Main';\n\nfunction LandingPage() {\n  \n  return (\n  <div className=\"container\">\n    <div className=\"title\">\n    <h1>Welcome to Group 2's library</h1>\n    </div>\n    <ul class=\"slide-right\">\n    <li><h2 class=\"science\">Science</h2></li>\n    <li><h2 class=\"technology\">Technology</h2></li>\n    <li><h2 class=\"business\">Business</h2></li>\n    <li><h2 class=\"self\">Self-improvement</h2></li>\n    <li><h2 class=\"slide-right economics\">Economics</h2></li>\n    <li><h2 class=\"slide-right biology\">Biology</h2></li>\n    <li><h2 class=\"slide-right coding\">Coding</h2></li>\n    </ul>\n    <div class=\"bookshelf_wrapper\">\n      <ul class=\"books_list\">\n        <li class=\"book_item first\"></li>\n        <li class=\"book_item second\"></li>\n        <li class=\"book_item third\"></li>\n        <li class=\"book_item fourth\"></li>\n        <li class=\"book_item fifth\"></li>\n        <li class=\"book_item sixth\"></li>\n     </ul>\n      <div class=\"shelf\"></div>\n    </div>\n    <NavLink to=\"/home\"><button className=\"enter-button\">Enter</button></NavLink>\n  </div>\n  )\n}\n\nexport default LandingPage;\n","/Users/korni/Documents/Arbeit/coding/React/20FullStackGroupWork/fullstack-book-app/src/components/Header.js",[],"/Users/korni/Documents/Arbeit/coding/React/20FullStackGroupWork/fullstack-book-app/src/components/InfoContainer.js",[],"/Users/korni/Documents/Arbeit/coding/React/20FullStackGroupWork/fullstack-book-app/src/client.js",[],"/Users/korni/Documents/Arbeit/coding/React/20FullStackGroupWork/fullstack-book-app/src/components/Card.js",["54"],"import React, { useState } from \"react\";\nimport \"./Card.css\";\n\nfunction Card({ card, image }) {\n  const [copySuccess, setCopySuccess] = useState(\"\");\n\n  const createObject = () => {\n    return {\n      title: card.volumeInfo.title,\n      authors: card.volumeInfo.authors,\n      categories: card.volumeInfo.categories,\n      pages: card.volumeInfo.pageCount,\n      date: card.volumeInfo.publishedDate,\n      thumbnail: card.volumeInfo.imageLinks.smallThumbnail,\n    };\n  };\n\n  const copyToClipBoard = async () => {\n    const objString = JSON.stringify(createObject());\n    try {\n      await navigator.clipboard.writeText(objString);\n      setCopySuccess(\"Copied!\");\n    } catch (err) {\n      setCopySuccess(\"Failed to copy!\");\n    }\n  };\n\n  return (\n    <div className=\"Card__card-container\">\n      <p>{card.volumeInfo.title && card.volumeInfo.title}</p>\n      <p>\n        {card.volumeInfo.authors\n          ? card.volumeInfo.authors.length > 1\n            ? card.volumeInfo.authors.join(\" & \")\n            : card.volumeInfo.authors\n          : \"No Author\"}\n      </p>\n      <p>\n        Categories:\n        {card.volumeInfo.categories\n          ? card.volumeInfo.categories\n          : \"No Categories\"}\n      </p>\n      <p>\n        Pages:\n        {card.volumeInfo.pageCount\n          ? card.volumeInfo.pageCount\n          : \"No Page Count\"}\n      </p>{\" \"}\n      <p>\n        Date:\n        {card.volumeInfo.publishedDate\n          ? card.volumeInfo.publishedDate\n          : \"No Date\"}\n      </p>\n      <a href={card.volumeInfo.previewLink} target=\"_blank\">\n        Preview\n      </a>\n      <img\n        src={image && image}\n        alt=\"Thumbnail\"\n      />\n      <button onClick={copyToClipBoard}>Copy Object</button>\n      {copySuccess}\n    </div>\n  );\n}\n\nexport default Card;\n",{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line